BASE <http://example.com/>PREFIX ex: <http://example.com/>PREFIX exb: <http://example.com/books/>PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>CONSTRUCT {  ?URI a ex:Buch;      ex:Titel ?Titel;      ex:Seiten ?Seiten;      ex:Jahr ?Jahr;      ex:Auflage ?Auflage;      ex:Preis ?Preis;      ex:ISBN ?ISBN;      ex:URNISBN ?UISBN;      ex:Verlag_ID ?Verlag_ID;#  ?URI a ex:Buch;#      ex:Titel ?a;#      ex:Seiten ?b;#      ex:Jahr ?c;#      ex:Auflage ?d;#      ex:Preis ?e;#      ex:ISBN ?f;#      ex:URNISBN ?g;#      ex:Verlag_ID ?h;}FROM <file:buch.csv>WHERE {  BIND(REPLACE(?Titel, ',', '-') AS ?URI1)  BIND(REPLACE(?URI1, ' ', '-') AS ?URI2)  BIND(REPLACE(?URI2, ':', '-') AS ?URI3)  BIND (URI(CONCAT('books/', REPLACE(?URI3, '--', '-'))) AS ?URI)#  BIND (URI(CONCAT('books/', REPLACE(?Titel, ':', '-'))) AS ?URI).#  REPLACE (?URI, ',', '-'). #&& REPLACE (?URI, '', '-')#  BIND (URI(CONCAT('', ?Titel)) AS ?URI)  BIND (CONCAT('URN:ISBN:',?ISBN) AS ?UISBN)#  BIND (URI(CONCAT('books/', ?a)) AS ?URI)#  BIND (CONCAT('URN:ISBN:',?g) AS ?UISBN)#  BIND (URI(CONCAT('companies/', ?Stock_ticker)) AS ?URI)#  BIND (STRLANG(?Name, "en") AS ?NameWithLang)}OFFSET 1